; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "InputsBase",
"language": "GDScript",
"path": "res://BaseClasses/InputsBase.gd"
}, {
"base": "Control",
"class": "MenuBase",
"language": "GDScript",
"path": "res://BaseClasses/GameMenuBase.gd"
}, {
"base": "Button",
"class": "MenuOptionsBase",
"language": "GDScript",
"path": "res://BaseClasses/MenuOptionsBase.gd"
}, {
"base": "Node2D",
"class": "Player",
"language": "GDScript",
"path": "res://Scenes/Player/Scripts/Player.gd"
} ]
_global_script_class_icons={
"InputsBase": "",
"MenuBase": "",
"MenuOptionsBase": "",
"Player": ""
}

[application]

config/name="Code And Run"
run/main_scene="res://Scenes/Levels/Debug/DebugLevel.tscn"
config/icon="res://icon.png"

[display]

window/size/width=960
window/size/height=540
window/size/borderless=true
window/size/fullscreen=true
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
quality/shadows/filter_mode=0
environment/default_environment="res://default_env.tres"
